$PREF{account_activated_email_subject}			= qq`Account activated on $ENV{HTTP_HOST}`;
$PREF{account_activated_email_template}			= qq`Hello,\n\nYour new account with username '%%username%%' has been approved and activated on $ENV{HTTP_HOST}.\n\nThanks,\n%PREF{title_text}\n`;
#
$PREF{notify_admin_of_new_signups}			= 'yes';
